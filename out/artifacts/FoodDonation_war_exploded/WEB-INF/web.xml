<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>logic</servlet-name>
        <servlet-class>WebApp.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logic</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>signup1</servlet-name>
        <servlet-class>WebApp.signup1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup1</servlet-name>
        <url-pattern>/signup1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>offer_ride</servlet-name>
        <servlet-class>WebApp.offer_ride</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>offer_ride</servlet-name>
        <url-pattern>/offer_ride</url-pattern>
    </servlet-mapping>
</web-app>